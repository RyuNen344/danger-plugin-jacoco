{
  "name": "@ryunen344/danger-plugin-jacoco",
  "version": "0.0.1",
  "description": "Dnager JS plugin for jacoco",
  "author": "RyuNen344",
  "repository": "https://github.com/RyuNen344/danger-plugin-jacoco",
  "bugs": "https://github.com/RyuNen344/danger-plugin-jacoco/issues",
  "homepage": "https://github.com/RyuNen344/danger-plugin-jacoco#readme",
  "keywords": [
    "danger",
    "danger-plugin",
    "jacoco"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "npm": "⚠️ plz use yarn instead ⚠️",
    "node": ">=16.14.0"
  },
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:dev": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "test": "jest",
    "coverage": "jest --coverage",
    "predocs": "rm -rf docs/",
    "docs": "typedoc",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "^@/(.+)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "coverageDirectory": "coverage"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "danger": "^11.0.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths": "^3.13.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "fast-xml-parser": "^4.0.6",
    "tslog": "^3.3.2"
  }
}
